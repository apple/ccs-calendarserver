#PYTHONPATH

if __name__ == "__main__":

    if "PYTHONPATH" in globals():
        sys.path.insert(0, PYTHONPATH)
    else:
        try:
            import _calendarserver_preamble
        except ImportError:
            sys.exc_clear()

