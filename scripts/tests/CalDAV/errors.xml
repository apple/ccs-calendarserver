					<arg>
						<name>error</name>
						<value>{urn:ietf:params:xml:ns:caldav}valid-calendar-object-resource</value>
					</arg>
					<arg>
						<name>ignoreextras</name>
						<value>true</value>
					</arg>
				</verify>
			</request>
		</test>
